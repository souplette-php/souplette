{
  "name": "ju1ius/souplette",
  "description": "Standard compliant HTML parser.",
  "type": "library",
  "version": "0.0.1",
  "license": "MIT",
  "authors": [
    {
      "name": "ju1ius",
      "email": "jules.bernable@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Souplette\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Souplette\\Tests\\": "tests",
      "Souplette\\Codegen\\": "codegen"
    }
  },
  "require": {
    "php": ">=8.1",
    "ext-ctype": "*",
    "ext-intl": "*",
    "ext-mbstring": "*"
  },
  "suggest": {
    "ext-xmlreader": "To parse XML documents into a DOM tree."
  },
  "require-dev": {
    "ext-json": "*",
    "ext-xmlreader": "*",
    "twig/twig": "^3.0",
    "blackfire/php-sdk": "^1.21"
  }
}
